println : cfunc(format : string, ...) -> null;

Date : struct {
	day : int;
	month : int;
	year : int;
}

Person : struct {
	name : string;
	birthday : Date;
}

print_person : (p : Person) -> null {
	println("test %s", p.name);
}

main : () -> null {
	
	david : Person;
	david.name = "David";
	david.birthday.day = 24;
	david.birthday.month = 1;
	david.birthday.year = 2000;

	println(
		"%s was born on %d/%d/%d!", 
		david.name,
		david.birthday.day,
		david.birthday.month,
		david.birthday.year
	);

	print_person(david);
		
}
